{
  "name": "n8ndoc_br",
  "version": "1.0.0",
  "description": "Documentação completa do n8n em português brasileiro - Hub de conhecimento para a comunidade brasileira de automação.",
  "main": "index.js",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --port 3000",
    "start:clean": "npm run kill-port && docusaurus start --port 3000",
    "start:alt": "docusaurus start --port 3001",
    "kill-port": "npx kill-port 3000 3001 3002 4000",
    "dev": "npm run kill-port && docusaurus start --port 3000",
    "docs:dev": "docusaurus start --port 3000",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "deploy:pages": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 20",
    "markdown:lint": "markdownlint-cli2 \"docs/**/*.{md,mdx}\" \"README.md\" \"CONTRIBUTING.md\"",
    "ci:validate": "npm run lint:check",
    "ci:validate-full": "npm run lint:check && npm run typecheck",
    "ci:build": "npm run ci:validate && npm run build",
    "preview": "npm run build && npm run serve",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:serve": "storybook dev -p 6006 --ci",
    "fix-sensitive-content": "node scripts/fix-sensitive-content.js",
    "security:check": "node scripts/fix-sensitive-content.js --check",
    "security:scan": "gitleaks detect --config .gitleaks.toml --verbose",
    "security:full": "npm run security:check && npm run security:scan",
    "deps:monitor": "node scripts/monitor-dependencies.js full",
    "deps:critical": "node scripts/monitor-dependencies.js critical",
    "deps:minor": "node scripts/monitor-dependencies.js minor",
    "deps:performance": "node scripts/monitor-dependencies.js performance",
    "deps:report": "node scripts/monitor-dependencies.js full --save",
    "prepare": "echo 'Skipping husky install'",
    "progress:update": "node scripts/update-progress-post.js"
  },
  "keywords": [
    "n8n",
    "documentação",
    "automação",
    "workflow",
    "português"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/module-type-aliases": "^3.8.1",
    "@docusaurus/plugin-client-redirects": "^3.8.1",
    "@docusaurus/plugin-content-blog": "^3.8.1",
    "@docusaurus/plugin-content-docs": "^3.8.1",
    "@docusaurus/plugin-content-pages": "^3.8.1",
    "@docusaurus/plugin-debug": "^3.8.1",
    "@docusaurus/plugin-ideal-image": "^3.8.1",
    "@docusaurus/plugin-pwa": "^3.8.1",
    "@docusaurus/plugin-svgr": "^3.8.1",
    "@docusaurus/preset-classic": "^3.8.1",
    "@docusaurus/remark-plugin-npm2yarn": "^3.8.1",
    "@docusaurus/theme-common": "^3.8.1",
    "@docusaurus/theme-live-codeblock": "^3.8.1",
    "@docusaurus/theme-mermaid": "^3.8.1",
    "@docusaurus/tsconfig": "^3.8.1",
    "@docusaurus/types": "^3.8.1",
    "@easyops-cn/docusaurus-search-local": "^0.52.1",
    "clsx": "^2.1.1",
    "docusaurus-theme-github-codeblock": "^2.0.2",
    "katex": "^0.16.22",
    "medium-zoom": "^1.1.0",
    "prism-react-renderer": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark-docusaurus-tabs": "^0.2.0",
    "restructured": "^0.0.11",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/addon-themes": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@types/node": "^22.10.2",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.37.5",
    "identity-obj-proxy": "^3.0.0",
    "kill-port": "^2.0.1",
    "markdownlint-cli2": "^0.18.1",
    "typescript": "^5.8.3"
  },
  "overrides": {
    "rimraf": "^5.0.7",
    "glob": "^10.3.10",
    "inflight": "npm:@isaacs/caching-transform@^1.0.1",
    "webpack-dev-server": "^5.2.1",
    "@babel/plugin-proposal-private-property-in-object": "npm:@babel/plugin-transform-private-property-in-object@^7.24.1",
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.4.15",
    "core-js": "^3.35.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  },
  "resolutions": {
    "core-js": "^3.35.1",
    "acorn-dynamic-import": "^4.0.0",
    "source-map": "^0.8.0"
  }
}
