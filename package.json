{
  "name": "n8ndoc_br",
  "version": "1.0.0",
  "description": "Documentação completa do n8n em português brasileiro - Hub de conhecimento para a comunidade brasileira de automação.",
  "main": "index.js",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --port 3000",
    "start:clean": "npm run kill-port && docusaurus start --port 3000",
    "start:alt": "docusaurus start --port 3001",
    "kill-port": "npx kill-port 3000 3001 3002 4000",
    "dev": "npm run kill-port && docusaurus start --port 3000",
    "docs:dev": "docusaurus start --port 3000",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 20",
    "validate-overlaps": "node scripts/validate-overlaps.js",
    "optimize-images": "node scripts/optimize-images.js",
    "generate-meta": "node scripts/generate-meta.js",
    "check-seo": "node scripts/check-seo.js",
    "spell-check": "node scripts/spell-check.js",
    "format-check": "node scripts/format-check.js",
    "accessibility-check": "node scripts/accessibility-check.js",
    "check-broken-links": "node scripts/find-broken-links.js",
    "check-duplicate-routes": "node scripts/find-duplicate-routes.js",
    "check-broken-anchors": "node scripts/find-broken-anchors.js",
    "check-all-issues": "node scripts/check-all-issues.js",
    "fix-unicode-math": "node scripts/fix-unicode-math.js",
    "markdown:lint": "markdownlint-cli2 \"docs/**/*.{md,mdx}\" \"README.md\" \"CONTRIBUTING.md\"",
    "quality-check": "npm run check-seo && npm run spell-check && npm run format-check && npm run accessibility-check",
    "ci:validate": "npm run lint:check && npm run validate-overlaps",
    "ci:validate-full": "npm run lint:check && npm run typecheck && npm run validate-overlaps",
    "ci:build": "npm run ci:validate && npm run build",
    "preview": "npm run build && npm run serve"
  },
  "keywords": [
    "n8n",
    "documentação",
    "automação",
    "workflow",
    "português"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/faster": "^3.8.1",
    "@docusaurus/module-type-aliases": "^3.8.1",
    "@docusaurus/plugin-content-docs": "^3.8.1",
    "@docusaurus/preset-classic": "^3.8.1",
    "@docusaurus/theme-common": "^3.8.1",
    "@docusaurus/theme-mermaid": "^3.8.1",
    "@docusaurus/tsconfig": "^3.8.1",
    "@docusaurus/types": "^3.8.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@floating-ui/react": "^0.27.14",
    "@headlessui/react": "^2.2.6",
    "@iconify/react": "^6.0.0",
    "@loadable/component": "^5.16.7",
    "@mdx-js/react": "^3.1.0",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@types/react-responsive": "^8.0.8",
    "@vercel/analytics": "^1.5.0",
    "clsx": "^2.1.1",
    "docutils": "^1.2.1",
    "dotenv": "^17.2.0",
    "framer-motion": "^12.23.0",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0",
    "ionicons": "^8.0.10",
    "js-yaml": "^4.1.0",
    "katex": "^0.16.22",
    "prism-react-renderer": "^2.4.1",
    "react": "^18.3.1",
    "react-bits": "^1.0.5",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "react-intersection-observer": "^9.16.0",
    "react-rating-stars-component": "^2.2.0",
    "react-responsive": "^10.0.1",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0",
    "restructured": "^0.0.11",
    "sphinx": "^0.1.2"
  },
  "devDependencies": {
    "@octokit/rest": "^22.0.0",
    "@types/react": "^18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitalets/google-translate-api": "^9.2.1",
    "axios": "^1.10.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.37.5",
    "glob": "^10.3.10",
    "kill-port": "^2.0.1",
    "markdownlint-cli2": "^0.18.1",
    "typescript": "^5.8.3"
  },
  "overrides": {
    "rimraf": "^5.0.7",
    "glob": "^10.3.10",
    "inflight": "npm:@isaacs/caching-transform@^1.0.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
